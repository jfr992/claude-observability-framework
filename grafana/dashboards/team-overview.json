{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "type": "row",
      "title": "Organization Overview",
      "collapsed": false
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "type": "stat",
      "title": "Total Cost",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum(increase(claude_code_cost_usage_USD_total[$__range]))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 100 }, { "color": "red", "value": 500 }] } } }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 2,
      "type": "stat",
      "title": "Active Users",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "count(count by (user_email) (claude_code_cost_usage_USD_total)) or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] } } }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 3,
      "type": "stat",
      "title": "Cost / User",
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum(increase(claude_code_cost_usage_USD_total[$__range])) / (count(count by (user_email) (claude_code_cost_usage_USD_total)) > 0 or vector(1))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 20 }, { "color": "red", "value": 50 }] } } }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "type": "stat",
      "title": "Total PRs",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum(increase(claude_code_pull_request_count_total[$__range])) or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 1 },
      "id": 5,
      "type": "stat",
      "title": "Total Commits",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum(increase(claude_code_commit_count_total[$__range])) or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] } } }
    },
    {
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "type": "stat",
      "title": "Org Accept %",
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "100 * sum(max_over_time(claude_code_code_edit_tool_decision_total{decision=\"accept\"}[$__range])) / (sum(max_over_time(claude_code_code_edit_tool_decision_total[$__range])) > 0 or vector(1))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "percent", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "yellow", "value": 60 }, { "color": "green", "value": 80 }] } } }
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "type": "row",
      "title": "Developer Leaderboard",
      "collapsed": false
    },
    {
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 6 },
      "id": 10,
      "type": "table",
      "title": "Developer Metrics",
      "options": { "showHeader": true, "sortBy": [{ "displayName": "Cost", "desc": true }] },
      "targets": [
        { "expr": "sum by (user_email) (increase(claude_code_cost_usage_USD_total[$__range]))", "refId": "cost", "format": "table", "instant": true },
        { "expr": "sum by (user_email) (increase(claude_code_token_usage_tokens_total[$__range]))", "refId": "tokens", "format": "table", "instant": true },
        { "expr": "count by (user_email) (count by (user_email, session_id) (claude_code_cost_usage_USD_total))", "refId": "sessions", "format": "table", "instant": true },
        { "expr": "sum by (user_email) (increase(claude_code_pull_request_count_total[$__range]))", "refId": "prs", "format": "table", "instant": true },
        { "expr": "sum by (user_email) (increase(claude_code_commit_count_total[$__range]))", "refId": "commits", "format": "table", "instant": true },
        { "expr": "sum by (user_email) (increase(claude_code_lines_of_code_count_total{type=\"added\"}[$__range]))", "refId": "lines", "format": "table", "instant": true }
      ],
      "transformations": [
        { "id": "merge", "options": {} },
        { "id": "organize", "options": {
          "excludeByName": { "Time": true },
          "renameByName": {
            "user_email": "Developer",
            "Value #cost": "Cost",
            "Value #tokens": "Tokens",
            "Value #sessions": "Sessions",
            "Value #prs": "PRs",
            "Value #commits": "Commits",
            "Value #lines": "Lines Added"
          }
        }}
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "Cost" }, "properties": [{ "id": "unit", "value": "currencyUSD" }] },
          { "matcher": { "id": "byName", "options": "Tokens" }, "properties": [{ "id": "unit", "value": "short" }] },
          { "matcher": { "id": "byName", "options": "Developer" }, "properties": [{ "id": "custom.width", "value": 250 }] }
        ]
      }
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "id": 102,
      "type": "row",
      "title": "Cost Analysis",
      "collapsed": false
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
      "id": 20,
      "type": "timeseries",
      "title": "Cost by Developer (over time)",
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["sum"] }, "tooltip": { "mode": "multi" } },
      "targets": [{ "expr": "sum by (user_email) (rate(claude_code_cost_usage_USD_total[1h])) * 3600", "legendFormat": "{{user_email}}", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD", "custom": { "lineWidth": 2, "fillOpacity": 20, "stacking": { "mode": "normal" } } } }
    },
    {
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
      "id": 21,
      "type": "piechart",
      "title": "Cost Distribution",
      "options": { "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }, "pieType": "donut", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum by (user_email) (increase(claude_code_cost_usage_USD_total[$__range]))", "legendFormat": "{{user_email}}", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" } }
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 25 },
      "id": 103,
      "type": "row",
      "title": "Usage Patterns",
      "collapsed": false
    },
    {
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 26 },
      "id": 30,
      "type": "piechart",
      "title": "By Model",
      "options": { "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }, "pieType": "donut", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum by (model) (increase(claude_code_cost_usage_USD_total[$__range]))", "legendFormat": "{{model}}", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" } }
    },
    {
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 26 },
      "id": 31,
      "type": "piechart",
      "title": "By Terminal/IDE",
      "options": { "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }, "pieType": "donut", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum by (terminal_type) (increase(claude_code_cost_usage_USD_total[$__range]))", "legendFormat": "{{terminal_type}}", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD" } }
    },
    {
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 26 },
      "id": 32,
      "type": "piechart",
      "title": "Token Distribution",
      "options": { "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }, "pieType": "donut", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum by (type) (increase(claude_code_token_usage_tokens_total[$__range]))", "legendFormat": "{{type}}", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 34 },
      "id": 104,
      "type": "row",
      "title": "Individual Developer (use variable filter)",
      "collapsed": false
    },
    {
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 35 },
      "id": 40,
      "type": "stat",
      "title": "Selected User Cost",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum(increase(claude_code_cost_usage_USD_total{user_email=~\"$user\"}[$__range]))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "currencyUSD", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 35 },
      "id": 41,
      "type": "stat",
      "title": "Selected User Sessions",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "count(count by (session_id) (claude_code_cost_usage_USD_total{user_email=~\"$user\"}))", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] } } }
    },
    {
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 35 },
      "id": 42,
      "type": "stat",
      "title": "Selected User PRs",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum(increase(claude_code_pull_request_count_total{user_email=~\"$user\"}[$__range])) or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } } }
    },
    {
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 35 },
      "id": 43,
      "type": "stat",
      "title": "Selected User Commits",
      "options": { "colorMode": "value", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [{ "expr": "sum(increase(claude_code_commit_count_total{user_email=~\"$user\"}[$__range])) or vector(0)", "refId": "A" }],
      "fieldConfig": { "defaults": { "unit": "short", "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] } } }
    },
    {
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 41 },
      "id": 44,
      "type": "timeseries",
      "title": "Selected User Activity",
      "options": { "legend": { "displayMode": "table", "placement": "right", "calcs": ["sum"] }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "sum(rate(claude_code_cost_usage_USD_total{user_email=~\"$user\"}[1h])) * 3600", "legendFormat": "Cost/hr", "refId": "A" },
        { "expr": "sum(rate(claude_code_token_usage_tokens_total{user_email=~\"$user\"}[1h]))", "legendFormat": "Tokens/hr", "refId": "B" }
      ],
      "fieldConfig": { "defaults": { "custom": { "lineWidth": 2, "fillOpacity": 20 } } }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["claude-code", "team", "multi-user"],
  "templating": {
    "list": [
      {
        "name": "user",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(claude_code_cost_usage_USD_total, user_email)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "time": { "from": "now-7d", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Claude Code - Team Overview",
  "uid": "claude-code-team",
  "version": 1
}
